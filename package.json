{
  "name": "angular-autofocus-fix--workspace",
  "version": "0.0.0",
  "engines": {
    "node": ">=8.0.0 <13.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve angular-8-test",
    "serve:prod": "npm run serve -- --aot --prod=true",
    "build": "ng build ngx-autofocus-fix",
    "test": "ng test ngx-autofocus-fix --browsers=ChromeHeadlessCI",
    "test:ci": "npm run test -- --no-watch --no-progress",
    "coveralls": "cat ./coverage/ngx-autofocus-fix/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:prod": "npm run e2e -- --prod=true",
    "e2e-install": "cd projects/angular-8-test && npm i && cd ../angular-7-test && npm i && cd ../angular-6-test && npm i && cd ../angular-5-test && npm i",
    "prepare-ver": "ts-node -P ./tools/tsconfig.json ./tools/prepare-version.ts",
    "publish": "npm publish dist/ngx-autofocus-fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
