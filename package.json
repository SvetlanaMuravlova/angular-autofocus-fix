    "@angular/language-service": "~11.2.14",
    "@types/jasmine": "~3.7.7",
    "@types/jasminewd2": "2.0.9",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "coveralls": "~3.1.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "wallaby-webpack": "^3.9.15"
  }
